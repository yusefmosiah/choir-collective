{
  "name": "template-next-tailwind-basic",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "anchor": "cd anchor && anchor",
    "anchor-build": "cd anchor && anchor build",
    "anchor-localnet": "cd anchor && anchor localnet",
    "dev": "concurrently \"pnpm run next-dev\" \"pnpm run fastapi-dev\"",
    "next-dev": "next dev",
    "fastapi-dev": "cd api && source venv/bin/activate && uvicorn main:app --reload",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest ",
    "test:py": "cd api && source venv/bin/activate && python -m pytest",
    "test:watch": "jest --watch",
    "test:anchor": "cd anchor && anchor test",
    "test:pynext": "concurrently \"cd api && source venv/bin/activate && python -m pytest\" \"pnpm run test\"",
    "test:all": "concurrently \"pnpm run test:anchor\" \"pnpm run test\" \"cd api && source venv/bin/activate && python -m pytest\""
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/spl-token": "^0.4.9",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.35",
    "@solana/web3.js": "^1.95.4",
    "@tabler/icons-react": "^3.20.0",
    "@tanstack/react-query": "^5.59.16",
    "@tanstack/react-query-next-experimental": "^5.59.16",
    "bs58": "4.0.1",
    "daisyui": "^4.12.13",
    "jotai": "^2.10.1",
    "next": "14.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@types/bn.js": "^5.1.6",
    "@types/chai": "^4.3.0",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^9.1.0",
    "@types/node": "^20.17.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "chai": "^4.3.4",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.2.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mocha": "^9.0.3",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "ts-jest": "^29.2.5",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.6.3"
  },
  "packageManager": "pnpm@9.12.3",
  "resolutions": {
    "bs58": "4.0.1"
  },
  "pnpm": {
    "overrides": {
      "bs58": "4.0.1"
    }
  }
